
	/* #FFF8F8 */
Page {
		color: #5a2424;
		background-color: #FFF8F8;
		font-size: 32rpx;
}
.primary {
		color: #c88989;
}
.fillScreen {
		width: 100vw;
		height: 100vh;
}
.center {
		display: flex;
		flex-direction: column;
		align-items: center;
		justify-content: center;
}
.elevated {
		box-shadow: #d7d7d7 1rpx 1rpx 8rpx;
}
.elevated:active {
		box-shadow: #e6e6e6 1rpx 1rpx 3rpx;
}
.outlined {
		border: #fffcfc solid 0.1rpx;
}
.groundGlass {
		background-color: #FFFFFF55;
		-webkit-backdrop-filter: blur(15rpx);
		        backdrop-filter: blur(15rpx);
}
.button {
		color: #c88989;
		font-weight: 600;
		border-radius: 50rpx;
		padding: 25rpx 30rpx;
		background-color: #fff5f5;
}
.button:active {
		background-color: #faf1f1;
}
.radio {
		color: #c88989;
		font-size: 26rpx;
		font-weight: 600;
		border-radius: 50rpx;
		width: 70rpx;
		height: 70rpx;
		background-color: #fff5f5;
}
.radioOn {
		color: #fff5f5;
		font-size: 26rpx;
		font-weight: 600;
		border-radius: 50rpx;
		width: 70rpx;
		height: 70rpx;
		background-color: #c88989;
}
